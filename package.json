{
  "name": "kintone-plugin-template",
  "version": "1.0.0",
  "scripts": {
    "create-ppk": "kintone-plugin-packer --out dist/plugin-dev.zip plugin",
    "move-ppk": "node scripts/move-ppk.ts",
    "init": "run-s develop create-ppk move-ppk",
    "bundle:dev": "webpack --config webpack.dev.js",
    "bundle:prod": "webpack --config webpack.prod.js",
    "bundle:watch": "webpack --config webpack.dev.js -w",
    "merge-manifest:dev": "ts-node scripts/marge-json.ts dev",
    "merge-manifest:prod": "ts-node scripts/marge-json.ts prod",
    "develop": "run-s merge-manifest:dev bundle:dev",
    "build": "run-s  merge-manifest:prod bundle:prod",
    "watch": "run-s merge-manifest:dev bundle:watch",
    "upload": "kintone-plugin-uploader dist/plugin-dev.zip --watch --waiting-dialog-ms 3000",
    "start": "run-p watch upload"
  },
  "devDependencies": {
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@kintone/dts-gen": "6.1.3",
    "@kintone/plugin-packer": "6.0.4",
    "@kintone/plugin-uploader": "7.0.0",
    "@kintone/rest-api-client": "3.1.3",
    "@kintone/webpack-plugin-kintone-plugin": "6.0.4",
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "@types/luxon": "2.3.2",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "esbuild-loader": "2.19.0",
    "fork-ts-checker-webpack-plugin": "7.2.11",
    "immer": "9.0.15",
    "luxon": "2.4.0",
    "notistack": "2.0.5",
    "npm-check-updates": "13.0.3",
    "npm-run-all": "4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recoil": "0.7.4",
    "terser-webpack-plugin": "5.3.3",
    "ts-loader": "9.3.1",
    "ts-node": "10.8.1",
    "typescript": "4.7.4",
    "webpack": "5.72.1",
    "webpack-cli": "4.9.2",
    "webpack-merge": "5.8.0"
  }
}
